{
    "version": 2,
    "builds": [
        { "src": "package.json", "use": "@vercel/next" }
    ],
    "routes": [
        { "src": "/_next/static/(.*)", "headers": { "cache-control": "public, max-age=31536000, immutable" } },
        { "src": "/(.*)", "dest": "/$1" }
    ],
    "framework": "nextjs"
}